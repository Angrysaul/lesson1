numbers = [3, 5, 7, 9, 10.5]
print(numbers)
numbers.append("Python")
print(numbers)
print(len(numbers))
print(numbers[0])
print(numbers[-1])
print(numbers[2:5])
numbers.remove("Python")
print(numbers)
phones = ["iphone", "nokia", "xiaomi"]
print(phones)
print(len(phones))
phones.append("samsung")
print(phones)
print("nokia " + str(phones.count("nokia")))
print("redmi " + str(phones.count("redmi")))

location = {
    "city": "Москва",
     "temperature": "20",
}
print(location["city"])
location["temperature"] = "15"
print(location)
print(location.get("country", "Russia"))
location["date"] = "27.05.2019"
print(len(location))
print(location)


stock = [
    {'name': 'iPhone Xs Plus', 'stock': 24, 'price': 65432.1, 
     'recomended': ['Samsung Galaxy S10', 'iPhone Xs']},
    {'name': 'Samsung Galaxy S10', 'stock': 8, 'price': 50000.0, 'discount': 5000},
    {'name': 'Xiaomi Mi8', 'stock': 42, 'price': 38000.5}
]
stock[0]["stock"] -= 5
print(stock)

